{
  "name": "oskconnectdb-functions",
  "description": "Cloud Functions for OSK Connect Database System",
  "scripts": {
    "serve": "firebase emulators:start --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "emulators": "firebase emulators:start",
    "emulators:export": "firebase emulators:export firebase-export.json",
    "emulators:import": "firebase emulators:start --import firebase-export.json",
    "emulators:clean": "firebase emulators:start --export-on-exit firebase-export.json"
  },
  "engines": {
    "node": "18"
  },
  "main": "index.js",
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "firebase-admin": "^11.11.1",
    "firebase-functions": "^4.9.0",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "firebase-functions-test": "^3.1.0"
  },
  "private": true
}